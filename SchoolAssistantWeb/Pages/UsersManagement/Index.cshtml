@page
@using Microsoft.AspNetCore.Antiforgery
@model SchoolAssistant.Web.Pages.UsersManagement.UsersManagementModel
@inject IAntiforgery antiforgery
@{
    var token = antiforgery.GetAndStoreTokens(HttpContext).RequestToken;
}

@section Head {
    <link rel="stylesheet" href="~/css/users-management/list.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/dist/users_management.css" asp-append-version="true"/>
}

@Html.React("Components.UsersCatalog", new {})

@section Scripts {
    <script>
        const antiforgeryToken = '@token';
    </script>

    <script src="~/dist/users_management.bundle.js" asp-append-version="true"></script>
}